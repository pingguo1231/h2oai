//TODO clone of frame-list-view -- refactor
.y-file-list-view
  // ko if:hasItems
  .y-files
    // ko foreach:items
    .y-file(data-bind='click:display, css:{"y-active":isActive}')
      .y-file-title(data-bind='text:title')
      .y-file-caption(data-bind='text:caption')
      .y-file-cutline(data-bind='timeago:timestamp')
    // /ko

  .y-predicate
    .y-predicate-caption(data-bind='paragraph:predicateCaption')
    button.y-predicate-clear.btn.fa-btn.fa.fa-times-circle(data-bind='visible:canClearPredicate, click:clearPredicate' title='Clear')
  // /ko

  // ko ifnot:hasItems
  .y-no-files
    .y-middle No datasets to display.
  // /ko

  .y-import-file
    button.btn.btn-primary(data-bind='click:importFile')
      span.fa.fa-upload
      span &nbsp;Import...

