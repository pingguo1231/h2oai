    "stylus": "~0.43.0",
    "coffeelint": "~1.1.0",
    "nib": "~1.0.2",
    "groc": "~0.6.3",
    "lodash": "~2.4.1",
    "watchy": "~0.4.3",
    "request": "~2.34.0",
    "tape": "~2.12.3",
    "browserify": "~3.44.2",
    "faucet": "0.0.1",
    "jshint": "~2.5.0",
